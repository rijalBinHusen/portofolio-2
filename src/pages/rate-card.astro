<style>
    section {
        /* background-color: rgb(243 244 246); */
        margin-top: 2rem;
        border-radius: .5rem;
        border-top: dashed .23rem pink;
        border-bottom: dashed .23rem pink;
        padding-bottom: 1.5rem;
    }

    section p {
        font-weight: 300;
        color: #757575;

    }

    .rate-card-main {
        padding-top: 8px;
        padding-right: 4px;
        padding-left: 4px;
        margin-left: auto;
        margin-right: auto;
        max-width: 1440px;
    }

    .rate-card-header {
        margin-left: auto;
        margin-right: auto;
        max-width: 960px;
        text-align: center;
        margin-bottom: 16px;
        padding-right: .32rem;
        padding-left: .32rem;
    }

    .rate-card-header h2 {
        margin-bottom: 16px;
        font-size: 24px;
        letter-spacing: -0.16px;
        font-weight: 800;
        color: #2a2a2a;
    }

    @media (min-width: 768px) {
        section {
            margin-top: 8rem;
        }
        section p {
            font-size: 16px;
        }

        .rate-card-main {
            padding-top: 16px;
            padding-right: 6px;
            padding-left: 6px;
        }

        .rate-card-header {
            margin-bottom: 24px;
        }
    }
</style>